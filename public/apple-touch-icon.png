<!-- Bu dosya aslında PNG olmalı, sadece placeholder olarak SVG koyuyorum -->
<svg width="180" height="180" viewBox="0 0 180 180" fill="none" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#8B5FBF;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#6366F1;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#4F46E5;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="icon" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#FFFFFF;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#E5E7EB;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background with rounded corners for iOS -->
  <rect width="180" height="180" rx="40" fill="url(#bg)" />
  
  <!-- AI Brain/Neural Network Icon - scaled up -->
  <g fill="url(#icon)" transform="translate(90, 90)">
    <!-- Central hub -->
    <circle cx="0" cy="0" r="8" />
    
    <!-- Neural nodes -->
    <circle cx="-35" cy="-35" r="6" />
    <circle cx="35" cy="-35" r="6" />
    <circle cx="-35" cy="35" r="6" />
    <circle cx="35" cy="35" r="6" />
    <circle cx="-45" cy="0" r="4" />
    <circle cx="45" cy="0" r="4" />
    <circle cx="0" cy="-45" r="4" />
    <circle cx="0" cy="45" r="4" />
    
    <!-- Connections -->
    <g stroke="url(#icon)" stroke-width="4" fill="none" opacity="0.8">
      <line x1="0" y1="0" x2="-35" y2="-35" />
      <line x1="0" y1="0" x2="35" y2="-35" />
      <line x1="0" y1="0" x2="-35" y2="35" />
      <line x1="0" y1="0" x2="35" y2="35" />
      <line x1="0" y1="0" x2="-45" y2="0" />
      <line x1="0" y1="0" x2="45" y2="0" />
      <line x1="0" y1="0" x2="0" y2="-45" />
      <line x1="0" y1="0" x2="0" y2="45" />
    </g>
    
    <!-- Additional neural connections -->
    <g stroke="url(#icon)" stroke-width="2.5" fill="none" opacity="0.5">
      <line x1="-35" y1="-35" x2="0" y2="-45" />
      <line x1="35" y1="-35" x2="0" y2="-45" />
      <line x1="-35" y1="35" x2="0" y2="45" />
      <line x1="35" y1="35" x2="0" y2="45" />
      <line x1="-35" y1="-35" x2="-45" y2="0" />
      <line x1="-35" y1="35" x2="-45" y2="0" />
      <line x1="35" y1="-35" x2="45" y2="0" />
      <line x1="35" y1="35" x2="45" y2="0" />
    </g>
  </g>
</svg> 